{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=sit-db-server;Database=ParcelScanDb;User Id=sit_user;Password={EncryptedPassword};TrustServerCertificate=True;"
  },
  "Encrypted": "SITEncryptedPasswordValue",
  "ScanEventApi": {
    "BaseUrl": "https://sit-api.yourdomain.com/v1/scans/scanevents",
    "Limit": 100,
    "PollingDelayMs": 30000
  },
  "Config": {
    "WorkerId": "W_SIT"
  }
}
